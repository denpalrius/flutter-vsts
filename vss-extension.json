{
    "manifestVersion": 1,
    "id": "flutter-vsts",
    "publisher": "dennis-riungu",
    "version": "1.0.4",
    "name": "Flutter-VSTS",
    "description": "A Visual Studio Services extension to test and build android and ios applications built using Google's Flutter",
    "public": false,
    "targets": [{
        "id": "Microsoft.VisualStudio.Services"
    }],
    "categories": [
        "Build and release"
    ],   

    "tags": [
        "build",
        "test",
        "flutter",
        "android",
        "apk",
        "ios"
    ],
    "icons": {
        "default": "assets/flutter-logo.png"
    },
    "branding": {
        "color": "rgb(0, 145, 234)",
        "theme": "dark"
    },
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "files": [
        {
            "path": "tasks"
        }
    ],
    "scopes": [
        "vso.code_write",
        "vso.test_write"
    ],
    "demands": [
        "java",
        "AndroidSDK",
        "JDK"
    ],
    "contributions": [{
        "id": "flutter-vsts-contrib",
        "type": "ms.vss-distributed-task.task",
        "targets": [
            "ms.vss-distributed-task.tasks"
        ],
        "properties": {
            "name": "tasks"
        }
    }],
    "links": {
        "learn": {
            "uri": "https://github.com/denpalrius/FlutterVSTS/blob/master/README.md"
        },
        "repository": {
            "uri": "https://github.com/denpalrius/FlutterVSTS.git"
        },
        "license": {
            "uri": "https://github.com/denpalrius/FlutterVSTS/LICENSE"
        },
        "support": {
            "uri": "mailto: denpalrius@live.com"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/denpalrius/FlutterVSTS.git"
    }
}