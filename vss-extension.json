{
    "manifestVersion": 1,
    "id": "flutter",
    "publisher": "dennis-riungu",
    "version": "1.0.9",
    "name": "Flutter Test & Build",
    "description": "Test and build Android and iOS applications using Google's Flutter mobile SDK in VSTS/TFS",
    "public": false,
    "targets": [{
        "id": "Microsoft.VisualStudio.Services"
    }],
    "categories": [
        "Build and release"
    ],
    "tags": [
        "Flutter",
        "Build",
        "Test",
        "Android",
        "apk",
        "Apple",
        "iPhone",
        "iPad",
        "ios",
        "ipa",
        "Continuous Integration"
    ],
    "icons": {
        "default": "images/flutter-logo.png"
    },
    "branding": {
        "color": "#E5E5E5",
        "theme": "light"
    },
    "content": {
        "details": {
            "path": "README.MD"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "files": [
        {
            "path": "Tasks/flutterBuildAndroid"
        },
        {
            "path": "Tasks/flutterBuildiOS"
        },
        {
            "path": "Tasks/flutterTest"
        },
        {
            "path": "images", "addressable": true
        }
    ],
    "scopes": [
        "vso.code_write",
        "vso.test_write"
    ],
    "contributions": [
        {
            "id": "flutter-build-android",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Tasks/flutterBuildAndroid"
            }
        },
        {
            "id": "flutter-build-ios",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Tasks/flutterBuildiOS"
            }
        },
        {        
        "id": "flutter-test",
        "type": "ms.vss-distributed-task.task",
        "targets": [
            "ms.vss-distributed-task.tasks"
        ],
        "properties": {
            "name": "Tasks/flutterTest"
        }
    }],
    "links": {
        "learn": {
            "uri": "https://github.com/denpalrius/FlutterVSTS/blob/master/README.md"
        },
        "repository": {
            "uri": "https://github.com/denpalrius/FlutterVSTS.git"
        },
        "license": {
            "uri": "https://github.com/denpalrius/FlutterVSTS/LICENSE"
        },
        "support": {
            "uri": "mailto: denpalrius@live.com"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/denpalrius/FlutterVSTS.git"
    }
}